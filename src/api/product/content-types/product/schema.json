{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "descriptionShort": {
      "type": "text",
      "required": true
    },
    "descriptionLong": {
      "type": "text",
      "required": false
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "unitPrice": {
      "type": "biginteger",
      "required": true
    },
    "inventoryAvailable": {
      "type": "biginteger",
      "required": true,
      "min": "0"
    },
    "related_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review"
    },
    "shipping_rates": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::shipping-rate.shipping-rate"
    }
  }
}
